<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meliksahcakir.spacedelivery">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_shuttle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_shuttle_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpaceDelivery"
        android:name=".SpaceDeliveryApplication">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>